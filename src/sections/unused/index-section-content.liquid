<section class="index-section-contents" data-name="{{ section.settings.text__SectionName }}" data-section-id="{{ section.settings.text__SectionId }}" id="index-section-contents--{{ section.id }}">
	{{ section.settings.text__SectionContentTEST }} 
</section>


{% schema %}
{
	"presets": [
		{
			"category": "Building Blocks",
			"name": "Index Section Contents"
		}
	],
	"blocks": [
		{
			"type": "text",
			"name": "Section",
			"settings": [
				{
					"id": "selec__BlockType",
					"type": "select",
					"label": "Section Type",
					"options": [
						{
							"value": 0,
							"label": "Slideshow"
						},
						{
							"value": 1,
							"label": "Full Text"
						}
					]
				},
				{
					"id": "text__BlockId",
					"type": "text",
					"label": "ID",
					"info": "This ID should be the same text as the ID in the corresponding Index Section Component."
				}
			]
		}
	],
	"settings": [
		{
			"type": "text",
			"id": "text__SectionId",
			"label": "ID",
			"default": "section-id",
			"info": "This ID should be the same text as the ID in the corresponding Index Section."
		},
		{
			"type": "text",
			"id": "text__SectionContentTEST",
			"label": "Text Content",
			"default": "FOO"
		}
	]
}
{% endschema %}